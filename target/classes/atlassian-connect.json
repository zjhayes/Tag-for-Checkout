{
  "key": "${addon.key}",
  "baseUrl": "${addon.base-url}",
  "name": "Tag for Checkout",
  "description": "Manage your Checkout items faster with any standard USB barcode reader.",
  "vendor": {
    "name": "Mumo Systems",
    "url": "https://www.mumosystems.com"
  },
  "authentication": {
    "type": "jwt"
  },
  "lifecycle": {
    "installed": "/installed",
    "uninstalled": "/uninstalled",
    "enabled": "/enabled",
    "disabled": "/disabled"
  },
  "scopes": ["READ", "WRITE"],
  "modules": {
    "generalPages": [
      {
        "key": "thymeleaf-page-jira",
        "location": "system.top.navigation.bar",
        "name": {
          "value": "Thymeleaf"
        },
        "url": "/thymeleaf"
      }
    ],
    "jiraProjectPages": [
      {
        "key": "tag-for-checkout",
        "name": {
          "value": "Tag"
        },
        "url": "/tag?projectKey=${project.key}",
        "iconUrl": "/images/tag-icon-gray-sm.png",
        "weight": 101,
        "conditions": [
          {
            "condition": "user_is_logged_in"
          },
          {
            "condition": "servicedesk.is_agent"
          },
          {
            "condition": "project_type",
            "params": {
              "projectTypeKey": "service_desk"
            }
          },
          {
            "condition": "entity_property_equal_to",
            "params": {
              "entity": "project",
              "propertyKey": "mumosystems-checkout",
              "objectName": "isEnabled",
              "value": "true"
            }
          }
        ]
      }
    ]
  }
}
